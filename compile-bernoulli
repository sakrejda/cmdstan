    -Istan/lib/stan_math/lib/boost_1.60.0  \
    -Istan/lib/stan_math/lib/boost_1.60.0/boost/compatibility/cpp_c_headers \
    -Istan/lib/stan_math/lib/cvodes_2.8.2/include    \
    -I/usr/local/include \
    -x c++ \
    examples/bernoulli/bernoulli.hpp    \
    -include src/cmdstan/command.hpp   \
    -include boost/exception/diagnostic_information.hpp \
    -include boost/exception_ptr.hpp \
    -x none \
    stan/lib/stan_math/lib/cvodes_2.8.2/lib/libsundials_nvecserial.a    \
    stan/lib/stan_math/lib/cvodes_2.8.2/lib/libsundials_cvodes.a    

